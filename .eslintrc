{
  "parser": "babel-eslint",
  "plugins": [],
  "extends": ["airbnb", "prettier"],
  "env": {
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "arrow-body-style": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "no-console": 0
  },
  "settings": {
    "import/resolver": {
      "babel-module": {
        "extensions": ["*", ".js", ".jsx"]
      }
    }
  }
}
